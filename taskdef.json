{
  "executionRoleArn": "arn:aws:iam::501045672667:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "nodejs",
      "image": "501045672667.dkr.ecr.us-east-1.amazonaws.com/xianbridal-nodejs:master",
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "secretOptions": null,
        "options": {
          "awslogs-group": "/ecs/xianbridal",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        {
          "name": "CONTAINER_IMAGE",
          "value": "<CONTAINER_IMAGE_1>"
        }
      ]
    },
    {
      "name": "nginx",
      "image": "501045672667.dkr.ecr.us-east-1.amazonaws.com/xianbridal-nginx:master",
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "secretOptions": null,
        "options": {
          "awslogs-group": "/ecs/xianbridal",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "hostPort": 443,
          "protocol": "tcp",
          "containerPort": 443
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "xianbridal"
}
