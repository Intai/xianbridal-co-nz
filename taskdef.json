{
  "executionRoleArn": "arn:aws:iam::501045672667:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "xianbridal-nodejs",
      "image": "<CONTAINER_IMAGE_NODEJS>",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 8080,
          "protocol": "tcp",
          "containerPort": 8080
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/xianbridal",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "nodejs"
        }
      },
      "environment": [
        {
          "name": "CONTAINER_IMAGE",
          "value": "<CONTAINER_IMAGE_NODEJS>"
        }
      ]
    },
    {
      "name": "xianbridal-nginx",
      "image": "<CONTAINER_IMAGE_NGINX>",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 443,
          "protocol": "tcp",
          "containerPort": 443
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/xianbridal",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "nginx"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "xianbridal"
}
